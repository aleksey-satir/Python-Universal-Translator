event:
  args:
    - el
    - event_name
    - fun
  code: "{{args.el}}.addEventListener({{args.event_name}}, {{args.fun}})"
  type: 'None'

!!python/tuple [list, list, '+']:
  code: "{{l}}.concat({{r}})"
  type: "{{l_type}}"

!!python/tuple ['any', 'any', '//']:
  code: "Math.floor({{l}}/{{r}})"
  type: int

check:
  args:
    - id
  code: "document.getElementById({{args.id}}).checked"
  type: bool

!!python/tuple ['list', 'type', '*']: 
  code: "{{l}}"
  type: "list<{{r}}>"

print:
  alt_name: "console.log"
  type: "None"

input:
  alt_name: "prompt"
  type: "str"

len:
  code: "{{args._1}}.length"
  type: int

interval:
  alt_name: "setInterval"
  type: "None"

url_param:
  alt_name: "new URL(window.location.href).searchParams.get"
  type: str

get_by_id:
  alt_name: "document.getElementById"
  type: dom

!!python/tuple ['dom', 'any', '<=']:
  code: "{{l}}.innerHTMl = {{r}}"
  type: "None"

!!python/tuple ['any', 'list', 'in']:
  code: "{{r}}.includes({{l}})"
  type: bool

classes:
  list:
    lox:
      code: |-
        {%for i in range(5)%}lox
        {%endfor%}
      type: None
      
    index:
      alt_name: "indexOf"
      type: int
    append:
      alt_name: "push"
      type: "None"
  math:
    "__name__": "Math"
    "pi":
      alt_name: "PI"
      type: float
  json:
    "__name__": "JSON"
    dumps:
      alt_name: "stringify"
    loads:
      alt_name: "parse"